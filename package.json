{
	"name": "@discoin/scambio",
	"version": "0.0.0-development",
	"keywords": [
		"discoin",
		"discord",
		"discord-bot"
	],
	"homepage": "https://scambio.discoin.zws.im/",
	"bugs": {
		"url": "https://github.com/Discoin/scambio/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Discoin/scambio.git"
	},
	"license": "MIT",
	"author": {
		"name": "The Discoin Association"
	},
	"contributors": [
		{
			"name": "Jonah Snider",
			"email": "jonah@jonah.pw",
			"url": "https://jonahsnider.com"
		}
	],
	"main": "./tsc_output/src/index",
	"scripts": {
		"prebuild": "rimraf tsc_output",
		"build": "tsc",
		"predocs": "rimraf docs_out",
		"docs": "typedoc --out docs_out --entryPoints ./src/index.ts",
		"lint": "xo",
		"pretest": "rimraf coverage .nyc_output",
		"test": "nyc ava",
		"style": "prettier --check ."
	},
	"xo": {
		"overrides": [
			{
				"files": "src/**/*.test.ts",
				"rules": {
					"@typescript-eslint/prefer-readonly-parameter-types": "off"
				}
			}
		],
		"prettier": true,
		"rules": {
			"unicorn/prefer-node-protocol": "off"
		}
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"files": [
			"!tsc_output",
			"!docs_out",
			"!coverage",
			"!.nyc_output"
		],
		"require": [
			"ts-node/register",
			"source-map-support/register"
		]
	},
	"nyc": {
		"all": true,
		"extends": "@istanbuljs/nyc-config-typescript",
		"include": [
			"src/**/*.ts"
		],
		"reporter": [
			"lcov",
			"cobertura"
		]
	},
	"dependencies": {
		"ky": "^0.25.0",
		"ky-universal": "^0.8.0"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "1.0.1",
		"@types/node": "14.17.21",
		"ava": "3.15.0",
		"eslint-plugin-prettier": "4.0.0",
		"nock": "13.1.3",
		"nyc": "15.1.0",
		"prettier": "2.4.1",
		"rimraf": "3.0.2",
		"semantic-release": "18.0.0",
		"source-map-support": "0.5.20",
		"ts-node": "10.2.1",
		"type-fest": "2.3.4",
		"typedoc": "0.22.5",
		"typescript": "4.4.3",
		"xo": "0.45.0"
	},
	"peerDependencies": {
		"@nestjsx/crud-request": "^4.3.3"
	},
	"engines": {
		"node": ">=10"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.0.2"
}
